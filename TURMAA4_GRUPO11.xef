		<actionSource name="inicializar">
			<STSource>(* INICIALIZAR AS VARIAVEIS A ZERO*)

decide := true;

for i:= 1 to 10 do

	for j:= 0 to 3 do

		MPeca[i][j] := 0;

	end_for;
end_for;

MPeca[1][0] := 1;
</STSource>
