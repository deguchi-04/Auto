		<actionSource name="Ver_Cod_Barras">
			<STSource>(*SE CHEGAR UMA NOVA PECA ALGUMA DAS PECAS DISPONIVEIS RECEBE SUAS CARACTERISTICAS*)

Cod_valido_0 := ((X &gt; 64 and X &lt; 69) or (X = 79)) and ((Y &gt; 64 and Y &lt; 69) or (Y=79)) and ((Z &gt; 64 and Z &lt; 69) or (Z=79)) and ((W &gt; 64 and W &lt; 69) or (W=79)) ;

Cod_valido_1 := true;

if(Cod_valido_0) then

	if(X = 79 and (Y &lt; 69 or Z&lt;69 or W&lt;69)) then

		Cod_valido_1 := false;
	
	elsif(Y = 79 and (Z&lt;69 or W&lt;69)) then

		Cod_valido_1 := false;

	elsif(Z = 79 and (W &lt;69)) then
	
		Cod_valido_1 := false;
	end_if;

end_if;

Cod_valido_2 := true;

if(Cod_valido_0 and Cod_valido_1) then

	if((X = 65 or X=68) and (Y=66 or Y=67 or Z=66 or Z=67 or W=66 or W=67)) then

		Cod_valido_2 := false;

	elsif((X = 66 or X=67) and (Y=65 or Y=68 or Z=65 or Z=68 or W=65 or W=68)) then	

		Cod_valido_2 := false;

	end_if;

end_if;


if(decide and Cod_valido_0 and Cod_valido_1 and Cod_valido_2) then

	for i:= 2 to 10 do

		if(MPeca[i][0] = 0) then

			if(AVARIADO.X) then

				if(X = 67) then

					X := 70;
				end_if;
				if(Y = 67) then

					Y := 70;
				end_if;
				if(Z = 67) then

					Z := 70;
				end_if;
				if(W = 67) then

					W := 70;
				end_if;

			end_if;

			MPeca[i][0] := X;
			MPeca[i][1] := Y;
			MPeca[i][2] := Z;
			MPeca[i][3] := W;

			TpPeca[i] := 1;
			(*TpNumPeca_0[1] := i_1;*)
			decide := false;
			i := 11;

		end_if;
	end_for;
end_if;
</STSource>
		</actionSource>
		<actionSource name="Escolhe_Maquina_TP1">
			<STSource>(* TENHO Q FAZER CADA TAPETE SABER QUAL A SUA PECA*)

for j := 2 to 10 do

	if(TpPeca[j] = 1) then

		TpNumPeca[1] := j;

			if(MPeca[j][0] &lt;&gt; 1) then

				
				TpDest[1] := MPeca[j][0];
